
#include<stdio.h>
int main()
{
    int n;
    int x=0,b=0,c=0,d=0,e=0;
    scanf("%d",&n);
    if(n>0&&n<=1000)
    {
        int a[n],i;
        for(i=0;i<n;i++)
        {
            scanf("%d",&a[i]);
            if(a[i]>=90)
            {
                x++;
            }else if(a[i]>=80)
            {
                b++;
            }else if(a[i]>=70)
            {
                c++;
            }else if(a[i]>=60)
            {
                d++;
            }else
            {
                e++;
            }
        }
        printf("%d %d %d %d %d",x,b,c,d,e);
    }
    return 0;
}
