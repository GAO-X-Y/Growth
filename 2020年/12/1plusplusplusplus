
#include<stdio.h>
int main()
{
  double sum = 0;
  int i = 1;
  int N,count = 1;//count代表当前的数列项（1就是1项、2就是第二项...)
  scanf("%d",&N);
  for(count; count <= N; i += 2,count++)
  {
    if(count%2!=0)
      sum += 1.0 / i;
      else
       sum -=1.0/i;
    
  }
  printf("sum = %.6lf",sum);
  return 0;
}
